{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": [false],
    "max-line-length": [
      500
    ],
    "member-access": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  }
}
